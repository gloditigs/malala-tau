{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/wp-content/(.*)", "dest": "/public/wp-content/$1" },
    { "src": "/css/(.*)", "dest": "/public/css/$1" },
    { "src": "/api/(.*)", "dest": "api/index.js" },
    { "src": "/(.*)", "dest": "api/index.js" }
  ]
}